package oopBasic;

import java.util.List;
import java.util.ArrayList;

public class ArrayList {
    
    public static void main(String[] args) {
        
        List<Integer> arrayList =  new ArrayList<Integer>();
        
        // Example usage
        arrayList.add(1);
        arrayList.add(2);
        arrayList.add(3);

        System.out.println(arrayList);
    }
}
